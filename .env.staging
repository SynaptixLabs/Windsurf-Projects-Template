# {{PROJECT_NAME}} — Staging Environment
# Real secrets come from CI/CD vault — this is the SHAPE, not the values.
# Set actual values in Railway / Vercel / GitHub Secrets / etc.

APP_ENV=staging
DEBUG=false
LOG_LEVEL=info

SECRET_KEY=SET_IN_CI_VAULT

DATABASE_URL=SET_IN_CI_VAULT
DB_POOL_SIZE=10
DB_MAX_OVERFLOW=20

PORT=8080
PUBLIC_URL=https://staging.{{YOUR_DOMAIN}}

ALLOWED_ORIGINS=https://staging.{{YOUR_DOMAIN}}

ANTHROPIC_API_KEY=SET_IN_CI_VAULT
# OPENAI_API_KEY=SET_IN_CI_VAULT

LLM_MODEL=claude-sonnet-4-6

STORAGE_BACKEND=s3
AWS_ACCESS_KEY_ID=SET_IN_CI_VAULT
AWS_SECRET_ACCESS_KEY=SET_IN_CI_VAULT
AWS_S3_BUCKET={{PROJECT_NAME}}-staging
AWS_REGION=us-east-1

SENTRY_DSN=SET_IN_CI_VAULT
SENTRY_ENVIRONMENT=staging

# Feature flags (staging: match prod unless testing new flags)
# FEATURE_FLAG_X=false
